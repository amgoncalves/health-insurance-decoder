document.querySelector('#user-profile-holder').innerHTML="<%= escape_javascript(render(partial:'/home/profile'))%>";
$(document).ready(function(){
    $("#change_picture").hide();
    $("#new_plan").hide();
    $("#btn").click(function(e) {
        $("#change_picture").show();
        $("#btn").hide();
    });
    $("#add").click(function(f) {
        $("#new_plan").show();
        $("#add").hide();
    });
});
