<% if not current_user %>
  <%= render partial: "static_pages/no_login" %>
<% else %>
  <div class="mdl-grid">
    <div class="mdl-cell mdl-cell--4-col for-log-dropdown-override">
      <%= render partial: 'log_history' %>
    </div>
    <div class="mdl-cell mdl-cell--8-col white-bg">
      <%= render "calendar" %>
    </div>
  </div>
<% end %>

<%= javascript_include_tag 'slider'%>
<%= javascript_include_tag 'log_index'%>
