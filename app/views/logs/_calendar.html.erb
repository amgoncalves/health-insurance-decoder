<%= month_calendar events: @current_user.logs do |date, logs| %>
  <%= date.day %>
  <% logs.each do |log| %>
    <div id="symptom_log">
      <%= link_to Symptom.find(log.symptom_id).name, edit_log_path(log) %>
    </div>
  <% end %>
  <% if not @visits_today.nil? %>
	  <% @visits_today.each do |visit|%>
	  	<div id="visit_log">
	      <%= visit.vtype %>
	    </div>
	  <% end %>
  <% end %>

<% end %>
