<canvas id="myChart" width="50" height="50"></canvas>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.4.0/Chart.min.js"></script>
<script>
var ctx = document.getElementById("myChart").getContext('2d');
var dataMap = {};
<% Log.all.each do |log| %>
  var s = "<%= escape_javascript(Symptom.find(log.symptom_id).name) %>";
  // console.log(s);
  if (s in dataMap){
    dataMap[s] = dataMap[s]+1;
  }
  else{
    dataMap[s] = 1;
    console.log("new:"+s);
  }
<%end%>
  console.log(dataMap);
  console.log(Object.keys(dataMap));
  console.log(Object.keys(dataMap).map(function(key){return dataMap[key];}));

var myChart = new Chart(ctx, {

    type: 'radar',
    data: {
        labels: Object.keys(dataMap),
        datasets: [{
            label: 'Symptom Chart',
            data: Object.keys(dataMap).map(function(key){return dataMap[key];}),
            backgroundColor: 'rgba(75, 192, 192, 0.2)',
            borderColor: 'rgba(54, 162, 235, 1)',
            borderWidth: 1
        }]
    },
    options: {
        events: ["mousemove"],
        scales: {
          min: 0
        }
    }
});

</script>
